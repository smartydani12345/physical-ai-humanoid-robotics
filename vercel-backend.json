{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "env": {
    "COHERE_API_KEY": "$COHERE_API_KEY",
    "QDRANT_URL": "$QDRANT_URL",
    "QDRANT_API_KEY": "$QDRANT_API_KEY",
    "GEMINI_API_KEY": "$GEMINI_API_KEY",
    "NEON_URL": "$NEON_URL",
    "API_TOKEN": "$API_TOKEN"
  }
}